                            }
                        },
                        {
                            "type": "group",
                            "header": "Pinned Locations",
                            "content": {
                                "type": "recentpins",
                                "style": "wrap",
                                "h_alignment": "stretch",
                                "v_alignment": "stretch",
                                "orientation": "vertical",
                                "compact": true
