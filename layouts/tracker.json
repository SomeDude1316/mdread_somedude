{
    "shared_item_grid": {
        "type": "array",
        "orientation": "vertical",
        "margin": "0,0",
        "content": [
            {
                "type": "array",
                "orientation": "horizontal",
                "margin": "0,0",
                "content": [
                    {
                        "type": "itemgrid",
                        "item_margin": "6,1",
                        "h_alignment": "left",
                        "rows": [
                            ["charge", "anybeam", "anysuit", "anymissile", "storm"],
                            ["anyjump", "morph", "bomb", "cross", "pb"],
                            ["slide", "spider", "grapple", "speed", "screw"],
                            ["", "cloak", "flash", "radar", ""]
                        ]
                    },
                    {
                        "type": "array",
                        "orientation": "vertical",
                        "v_alignment": "center",
                        "margin": "5,0,0,0",
                        "content": [
                            {
                                "type": "itemgrid",
                                "margin": "0,0",
                                "item_margin": "2,0",
                                "h_alignment": "center",
                                "rows": [
                                    [ "white", "green", "yellow" ],
                                    [ "blue", "purple", "red" ],
                                    [ "qr", "x" ],
                                    [ "etank","epart" ]
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "tracker_default": {
        "type": "container",
        "background": "#0c151c",
        "content": {
            "type": "dock",
            "content": [
                {
                    "type": "dock",
