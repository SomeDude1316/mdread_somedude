{
    "shared_item_grid": {
        "type": "array",
        "orientation": "vertical",
        "margin": "0,0",
        "content": [
            {
                "type": "array",
                "orientation": "horizontal",
                "margin": "0,0",
                "content": [
                    {
                        "type": "itemgrid",
                        "item_margin": "6,1",
                        "h_alignment": "left",
                        "rows": [
                            ["charge", "anybeam", "anysuit", "anymissile", "storm"],
                            ["anyjump", "morph", "bomb", "cross", "pb"],
                            ["slide", "magnet", "grapple", "speed", "screw"],
                            ["", "cloak", "flash", "radar", ""],
                            ["white", "green", "yellow", "blue", "purple", "red"]
                        ]
                    }
                ]
            },
            {
                "type": "array",
                "orientation": "vertical",
                "v_alignment": "center",
                "margin": "5,0,0,0",
                "content": [
                    {
                        "type": "itemgrid",
                        "margin": "0,0",
                        "item_margin": "2,0",
                        "h_alignment": "center",
                        "rows": [
                            [ "etank","epart" ]
                        ]
                    }
                ]
            }
        ]
    },
    "tracker_default": {
        "type": "container",
        "background": "#000000",
        "content": {
            "type": "dock",
            "content": [
                {
                    "type": "dock",
                    "dock": "left",
                    "v_alignment": "stretch",
                    "content": [
                        {
                            "type": "group",
                            "header": "Items",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "shared_item_grid"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Pinned Locations",
                            "content": {
                                "type": "recentpins",
                                "style": "wrap",
                                "h_alignment": "stretch",
                                "v_alignment": "stretch",
                                "orientation": "vertical",
                                "compact": true
                            }
                        }
                    ]
                },
                {
                    "type": "tabbed",
                    "background": "#0c151c",
                    "tabs": [
                        {
                            "title": "Artaria",
                            "content": {
                                "type": "map",
                                "maps": [
                                    "artaria"
                                ]
                            }
                        },
